@model MovieRentalService.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li><b>Membership Type:</b> @Model.MembershipType.Name</li>
    @if (Model.BirthDate != null)
    {
    <li><b>Birthdate:</b> @Model.BirthDate.Value.ToShortDateString()</li>
    }
</ul>

